#include<iostream>
using namespace std;
int main()
{
    int a, num, positivo=0;
    cout<<"cuantos numeros quiere leer";
    cin>>a;
    for(int i=1;i<=a;i++)
    {
        cout<<"di numero";
        cin>>num;
        if(num>0)
        {
            num=1;
            positivo+=+1;
        }
    }
    cout<<positivo<<" son numeros postivos"<<endl;
    return 0;
}
